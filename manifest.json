{
    "manifest_version": 2,
    "name": "URL Converter for Devs",
    "version": "1.0.0",
    "description": "A chrome extension to convert current url using the provided regex",
    "icons": {
        "128": "icons/128.png",
        "48": "icons/48.png",
        "16": "icons/16.png"
    },
    "browser_action": {
        "default_icon": "icons/16.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "tabs",
        "notifications",
        "contextMenus"
    ]
}